# Environment variables for AppBot MCP Server
# Copy this file to .env and fill in your actual values

# AppBot API Configuration
APPBOT_API_BASE_URL=https://api.appbot.example.com
APPBOT_API_KEY=your_api_key_here
APPBOT_API_VERSION=v1

# Server Configuration
MCP_SERVER_NAME=appbot-mcp-server
MCP_SERVER_VERSION=1.0.0

# Optional: Rate limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=60

# Optional: Logging level (error, warn, info, debug)
LOG_LEVEL=info